{
  "kind": "collectionType",
  "collectionName": "headers",
  "info": {
    "singularName": "header",
    "pluralName": "headers",
    "displayName": "header",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "show_logo": {
      "type": "boolean",
      "default": true
    },
    "links_button": {
      "type": "dynamiczone",
      "components": [
        "molecules.button",
        "sections.link"
      ]
    },
    "menu_items": {
      "displayName": "menu_item",
      "type": "component",
      "repeatable": true,
      "component": "molecules.menu-item"
    },
    "layouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::layout.layout",
      "mappedBy": "header"
    },
    "banners": {
      "type": "component",
      "repeatable": false,
      "component": "sections.repeatable-banner"
    },
    "alert": {
      "displayName": "info-alert",
      "type": "component",
      "repeatable": false,
      "component": "molecules.info-alert"
    },
    "social_medias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::social-media.social-media",
      "inversedBy": "headers"
    }
  }
}
