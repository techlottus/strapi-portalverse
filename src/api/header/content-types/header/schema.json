    },
    "links": {
      "type": "component",
      "repeatable": true,
      "component": "sections.link"
    },
    "button": {
      "type": "component",
      "repeatable": false,
      "component": "molecules.button"
    },
    "banners": {
      "type": "component",
      "repeatable": true,
      "component": "sections.banner",
      "max": 2
    },
    "linkText": {
      "type": "string"
    },
    "href": {
      "type": "string"
    },
    "social_medias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::social-media.social-media"
