{
  "collectionName": "programs",
  "info": {
    "singularName": "program",
    "pluralName": "programs",
    "displayName": "Program",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "detail": {
      "type": "richtext"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "level": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::level.level",
      "inversedBy": "programs"
    },
    "knowledgeAreas": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::knowledge-area.knowledge-area",
      "inversedBy": "programs"
    },
    "campuses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::campus.campus"
    },
    "programCategory": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::program-category.program-category",
      "inversedBy": "programs"
    },
    "programModalities": {
      "type": "component",
      "repeatable": true,
      "component": "programs.modality-feature"
    },
    "summary": {
      "type": "richtext"
    },
    "admissionProfile": {
      "type": "richtext"
    },
    "graduateProfile": {
      "type": "richtext"
    },
    "learningResources": {
      "type": "richtext"
    },
    "evaluationMethods": {
      "type": "richtext"
    },
    "laborField": {
      "type": "richtext"
    },
    "brands": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::brand.brand"
    },
    "checkoutUrl": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "default": 0
    },
    "offerPrice": {
      "type": "decimal",
      "default": 0
    },
    "priceDetail": {
      "type": "richtext"
    },
    "openings": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::opening.opening",
      "mappedBy": "program"
    },
    "brochure": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "available": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": false
    },
    "certificationMessage": {
      "type": "text"
    },
    "discountPercentageText": {
      "type": "string",
      "default": "de descuento"
    },
    "discount": {
      "type": "integer"
    },
    "periodicity": {
      "type": "enumeration",
      "enum": [
        "mes",
        "año",
        "bimestre",
        "semestre",
        "trimestre",
        "cuatrimestre"
      ],
      "default": "mes"
    },
    "checkoutUrlText": {
      "type": "string"
    },
    "relatedPrograms": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::program.program"
    },
    "programPerks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::program-perk.program-perk"
    },
    "nombreProgramaSalesforce": {
      "type": "string"
    },
    "duration": {
      "type": "integer"
    },
    "durationUnit": {
      "type": "enumeration",
      "enum": [
        "horas",
        "días",
        "semanas",
        "meses",
        "años"
      ],
      "default": "horas"
    },
    "goals": {
      "type": "richtext"
    },
    "testimonials": {
      "type": "component",
      "repeatable": false,
      "component": "sections.testimonial-slider"
    },
    "academicTitleName": {
      "type": "string"
    },
    "certifications": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::certification.certification",
      "inversedBy": "programs"
    },
    "certificationsTitle": {
      "type": "string"
    },
    "certificationsDescription": {
      "type": "richtext"
    },
    "admissionRequirements": {
      "type": "richtext"
    },
    "price_list": {
      "type": "component",
      "repeatable": false,
      "component": "sections.prices"
    }
  },
  "kind": "collectionType"
}
